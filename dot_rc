export PATH="$HOME/bin:$HOME/.local/bin:$PATH"

# 危险操作
alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'

# 常用快捷
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'

# 常用git操作
alias g='git'
alias gs='git status -sb'
alias ga='git add'
alias gc='git commit'
alias gp='git push'
alias gl='git pull'
alias gd='git diff'
alias gco='git checkout'
alias gb='git branch'
